@model mvc.Models.CompanyAndOrganizationFeedback

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container detail-container">
    <h2>Details</h2>

    <div>
        <div class="feedback-detail-wrapper">
            <h4>Feedback on Companies And Organizations </h4>
            <div class="feedback-detail">
                <dl>

                    <dt>
                        @Html.DisplayNameFor(model => model.Date)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Date)
                    </dd>
                </dl>
                <dl>
                    <dt>
                        @Html.DisplayNameFor(model => model.Username)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Username)
                    </dd>
                </dl>
                <dl>
                    <dt>
                        @Html.DisplayNameFor(model => model.Heading)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Heading)
                    </dd>
                </dl>
                <dl>
                    <dt>
                        @Html.DisplayNameFor(model => model.CompanyName)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.CompanyName)
                    </dd>
                </dl>
                <dl>
                    <dt>
                        @Html.DisplayNameFor(model => model.Rating)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Rating)
                    </dd>
                </dl>
                <dl>
                    <dt>
                        @Html.DisplayNameFor(model => model.Feedback)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Feedback)
                    </dd>
                </dl>
                <dl>
                    <dt>
                        @Html.DisplayNameFor(model => model.Agree)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Agree)
                    </dd>
                </dl>
                <dl>
                    <dt>
                        @Html.DisplayNameFor(model => model.Disagree)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Disagree)
                    </dd>
                </dl>
            </div>
        </div>
    </div>
    <div>
        <a asp-action="Edit" asp-route-id="@Model.ID">Edit</a> |
        <a href="/Home/Organizations">Back to Emerging Technologies</a>
    </div>
</div>